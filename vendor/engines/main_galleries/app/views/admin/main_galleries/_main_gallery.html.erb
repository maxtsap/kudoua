<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(main_gallery) -%>">
  <span class='title'>
    <%= main_gallery.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), main_gallery_path(main_gallery),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_main_gallery_path(main_gallery),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_main_gallery_path(main_gallery),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => main_gallery.title),
        :method => :delete %>
  </span>
</li>
